<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.choerodon.agile.infra.mapper.TagCompareHistoryMapper">


    <select id="selectByAppServiceCodes" resultType="io.choerodon.agile.infra.dto.TagCompareHistoryDTO">
        select *
        from agile_tag_compare_history
        where organization_id = #{organizationId}
        and project_id = #{projectId}
        and app_service_code in
        <foreach collection="appServiceCodes" item="appServiceCode" open="(" separator="," close=")">
            #{appServiceCode}
        </foreach>
    </select>
</mapper>